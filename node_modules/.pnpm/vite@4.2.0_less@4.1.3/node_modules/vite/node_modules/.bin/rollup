#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/存储/project/text/luoxuanya.github.io/node_modules/.pnpm/rollup@3.20.6/node_modules/rollup/dist/bin/node_modules:/Volumes/存储/project/text/luoxuanya.github.io/node_modules/.pnpm/rollup@3.20.6/node_modules/rollup/dist/node_modules:/Volumes/存储/project/text/luoxuanya.github.io/node_modules/.pnpm/rollup@3.20.6/node_modules/rollup/node_modules:/Volumes/存储/project/text/luoxuanya.github.io/node_modules/.pnpm/rollup@3.20.6/node_modules:/Volumes/存储/project/text/luoxuanya.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/存储/project/text/luoxuanya.github.io/node_modules/.pnpm/rollup@3.20.6/node_modules/rollup/dist/bin/node_modules:/Volumes/存储/project/text/luoxuanya.github.io/node_modules/.pnpm/rollup@3.20.6/node_modules/rollup/dist/node_modules:/Volumes/存储/project/text/luoxuanya.github.io/node_modules/.pnpm/rollup@3.20.6/node_modules/rollup/node_modules:/Volumes/存储/project/text/luoxuanya.github.io/node_modules/.pnpm/rollup@3.20.6/node_modules:/Volumes/存储/project/text/luoxuanya.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rollup@3.20.6/node_modules/rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../../../../rollup@3.20.6/node_modules/rollup/dist/bin/rollup" "$@"
fi
