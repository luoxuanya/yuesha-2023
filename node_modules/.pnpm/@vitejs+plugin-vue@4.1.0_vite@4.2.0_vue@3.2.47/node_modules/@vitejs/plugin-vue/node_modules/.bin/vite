#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/存储/project/text/luoxuanya.github.io/node_modules/.pnpm/vite@4.2.0_less@4.1.3/node_modules/vite/bin/node_modules:/Volumes/存储/project/text/luoxuanya.github.io/node_modules/.pnpm/vite@4.2.0_less@4.1.3/node_modules/vite/node_modules:/Volumes/存储/project/text/luoxuanya.github.io/node_modules/.pnpm/vite@4.2.0_less@4.1.3/node_modules:/Volumes/存储/project/text/luoxuanya.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/存储/project/text/luoxuanya.github.io/node_modules/.pnpm/vite@4.2.0_less@4.1.3/node_modules/vite/bin/node_modules:/Volumes/存储/project/text/luoxuanya.github.io/node_modules/.pnpm/vite@4.2.0_less@4.1.3/node_modules/vite/node_modules:/Volumes/存储/project/text/luoxuanya.github.io/node_modules/.pnpm/vite@4.2.0_less@4.1.3/node_modules:/Volumes/存储/project/text/luoxuanya.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@4.2.0_less@4.1.3/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@4.2.0_less@4.1.3/node_modules/vite/bin/vite.js" "$@"
fi
